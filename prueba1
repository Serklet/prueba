#hola

print ("hola")




continuar=1
while continuar == 1:
    
    print ("Dime tu nota")
    nota = int (input ())
    if nota == 10 or nota == 9:
        print("Sobresaliente ")
    elif nota== 8 or nota == 7 : 
        print("Notable")
    elif nota== 6:
        print("bien")
    elif nota==5:
        print("Sufi")
    elif nota<5 and nota >-1:
        print("Suspenso, pringao")
    else :
        print("vuelve a la escuela que eso no existe")

    print(" Para volver a comprobar, pulse 1, para terminar pulse otra tecla")
    del continuar

    continuar = int (input ())
    

print("Adios")
